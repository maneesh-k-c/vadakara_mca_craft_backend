<!DOCTYPE html>
<html>
<%- include('./modules/head') %>

  <body class="skin-blue">
    <div class="wrapper">

      <%- include('./modules/header') %>

        <%- include('./modules/sidebar') %>

          <!-- Right side column. Contains the navbar and content of the page -->
          <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
              <h1>
                All Products
              </h1>
              <ol class="breadcrumb">
                <li><a href="/admin/"><i class="fa fa-dashboard"></i> Dashboard</a></li>
                <li class="active">Works</li>
              </ol>
            </section>

            <!-- Main content -->
            <section class="content">
              <div class="row">

                <div class="container">
                  <div class="box" style="margin-top: 30px;">
                    <div class="box-header" style="text-align: center;">
                      <h3 class="box-title" style="font-weight: 800; color: rgb(130, 130, 232);">Students added products list</h3>
                    </div><!-- /.box-header -->
                    <div class="box-body no-padding">
                      <table class="table table-striped">
                        <tr>
                          <th style="width: 10px">#</th>
                          <th>Product Name</th>
                          <th>Student Name</th>
                          <th>Mobile</th>
                          <th>Price</th>
                          <th>Description</th>
                        </tr>
                        <% if(users.length!=0){ 
                           for(i=0;i<users.length;i++){%>
                        <tr>
                          <td><%=i+1%></td>
                          <td><%= users[i].product_name%></td>
                          <td><%= users[i].student_name%></td>
                          <td>
                            <%= users[i].mobile%>
                          </td>
                          <td>
                            <%= users[i].price%>
                          </td>
                          <td>
                            <img src="<%= users[i].image[0]%>" height="50" width="50" alt="">
                            
                          </td>
                          <td><%= users[i].description%></td>
                        </tr>

                        <%}}else{ %>

                          <td colspan="6"
                              style="padding-top: 45px; text-align: center; font-size: large; height: 120px;">
                              No data found
                          </td>
                          <% }%>%>%>
                
                      </table>
                    </div><!-- /.box-body -->
                  </div>
                </div>

               


              </div>
          </div>

          </section><!-- /.content -->
    </div><!-- /.content-wrapper -->
    <footer class="main-footer">
      <div class="pull-right hidden-xs">

      </div>

    </footer>
    </div><!-- ./wrapper -->

    <!-- jQuery 2.1.3 -->
    <script src="../../plugins/jQuery/jQuery-2.1.3.min.js"></script>
    <!-- Bootstrap 3.3.2 JS -->
    <script src="../../bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <!-- Slimscroll -->
    <script src="../../plugins/slimScroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <!-- FastClick -->
    <script src='../../plugins/fastclick/fastclick.min.js'></script>
    <!-- AdminLTE App -->
    <script src="../../dist/js/app.min.js" type="text/javascript"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="../../dist/js/demo.js" type="text/javascript"></script>
  </body>

</html>